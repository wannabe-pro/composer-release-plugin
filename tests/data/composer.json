{
    "repositories": [
        {
            "type": "path",
            "url": "subpackage/"
        },

        {
            "type": "path",
            "url": "../../"
        }
    ],
    "require": {
        "wannabe-pro/composer-release-plugin": "0.0.1",
        "test/subpackage": "0.0.1"
    },
    "autoload": {
        "psr-4": {
            "Test\\": "src/"
        }
    },
    "extra": {
        "build-plugin": {
            "build": {
                "builder": "WannaBePro\\Composer\\Plugin\\Release\\CopyFileBuilder",
                "composer": {
                    "repositories": [
                        {
                            "type": "path",
                            "url": "C:\\Users\\User\\Desktop\\test2"
                        }
                    ],
                    "require": {
                        "test2": "0.0.1"
                    },
                    "autoload": {
                        "psr-4": {
                            "Test\\": "src"
                        }
                    }
                },
                "mapper": {
                    "/(.*)\\.php$/": "$1.inc"
                }
            }
        }
    }
}
